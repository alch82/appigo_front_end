{
  "name": "todo-desktop",
  "version": "10.0.0-beta.5i",
  "description": "Todo Cloud",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build --target=development --environment=dev",
    "prepare": "cp -a electron/. dist",
    "build-electron": "ng build --target=development --environment=elec && npm run prepare",
    "electron": "npm run build-electron && electron dist/",
    "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist \"Todo Cloud (DEV)\" --platform=darwin --arch=all --out=dist-app --overwrite --icon=\"Todo-Cloud.icns\" --app-version=\"`cat package.json |grep \\\"\"version\\\"\" |cut -d \\\\\" -f4`\"",
    "start-test": "ng serve --target=development --environment=test",
    "build-test": "ng build --target=production --environment=test",
    "build-electron-test": "npm run build-test && npm run prepare",
    "electron-test": "npm run build-electron-test && electron dist",
    "package-test": "ng build --target=production --environment=elec && npm run prepare && ./node_modules/electron-packager/cli.js dist \"Todo Cloud (TEST)\" --platform=darwin -arch=all --out=dist-app --overwrite --icon=\"Todo-Cloud.icns\" --app-version=\"`cat package.json |grep \\\"\"version\\\"\" |cut -d \\\\\" -f4`\"",
    "start-beta": "ng serve --target=production --environment=beta",
    "build-beta": "ng build --target=production --environment=beta",
    "build-electron-beta": "npm run build-beta && npm run prepare",
    "electron-beta": "npm run build-electron-beta && electron dist",
    "package-beta": "npm run build-beta && npm run prepare && ./node_modules/electron-packager/cli.js dist \"Todo Cloud (BETA)\" --platform=darwin -arch=all --out=dist-app --overwrite --icon=\"Todo-Cloud.icns\" --app-version=\"`cat package.json |grep \\\"\"version\\\"\" |cut -d \\\\\" -f4`\"",
    "start-prod": "ng serve --target=production --environment=prod",
    "build-prod": "ng build --target=production --environment=prod",
    "build-electron-prod": "npm run build-prod && npm run prepare",
    "electron-prod": "npm run build-electron-prod && electron dist",
    "package-prod": "npm run build-prod && npm run prepare && ./node_modules/electron-packager/cli.js dist \"Todo Cloud\" --platform=darwin -arch=all --out=dist-app --overwrite --icon=\"Todo-Cloud.icns\" --app-version=\"`cat package.json |grep \\\"\"version\\\"\" |cut -d \\\\\" -f4`\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Appigo/todo-desktop.git"
  },
  "author": "Appigo, Inc.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Appigo/todo-desktop/issues"
  },
  "homepage": "https://github.com/Appigo/todo-desktop",
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.0",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@angular/upgrade": "^4.2.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.21",
    "@types/croppie": "^2.4.1",
    "@types/moment-timezone": "^0.2.34",
    "angular-linky": "^1.2.2",
    "angular2-contextmenu": "^0.8.2",
    "angular2-jwt": "^0.2.3",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "croppie": "^2.4.1",
    "dragula": "^3.7.2",
    "electron": "^1.6.11",
    "font-awesome": "^4.7.0",
    "google-libphonenumber": "^2.0.14",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.11",
    "ng2-dnd": "^4.2.0",
    "ng2-dragula": "^1.3.0",
    "ng2-toastr": "^4.1.2",
    "ngx-perfect-scrollbar": "^4.4.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "systemjs": "^0.20.14",
    "ts-helpers": "^1.1.2",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.2",
    "@angular/compiler-cli": "^4.2.4",
    "@types/core-js": "^0.9.35",
    "@types/googlemaps": "^3.26.11",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.5",
    "codelyzer": "~3.1.1",
    "electron-packager": "^8.5.0",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.1.2",
    "ts-node": "3.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.3.4"
  }
}
